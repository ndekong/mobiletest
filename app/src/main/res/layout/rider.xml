<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#292929"
    tools:context=".MainActivity">


    <LinearLayout
        android:id="@+id/linearLayout2"
        android:layout_width="0dp"
        android:layout_height="72dp"
        android:background="#101010"
        android:orientation="horizontal"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <TextView
            android:layout_width="101dp"
            android:layout_height="38dp"
            android:layout_marginLeft="20dp"
            android:layout_marginTop="16dp"
            android:fontFamily="sans-serif-black"
            android:text="Edvora"
            android:textColor="@color/white"
            android:textSize="32sp"
            android:textStyle="bold" />


        <TextView
            android:layout_width="97dp"
            android:layout_height="19dp"
            android:layout_marginLeft="109dp"
            android:layout_marginTop="10dp"
            android:text="Dhruv Singh"
            android:textColor="@color/white"
            android:textSize="16dp"
            android:textStyle="bold" />

    </LinearLayout>


    <TextView
        android:id="@+id/textView"
        android:layout_width="35dp"
        android:layout_height="15dp"
        android:layout_marginStart="20dp"
        android:layout_marginTop="86dp"
        android:text="Rides:"
        android:textColor="#ABABAB"
        android:textSize="12sp"
        android:textStyle="normal"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />


    <TextView
        android:id="@+id/textView2"
        android:layout_width="47dp"
        android:layout_height="15dp"
        android:layout_marginStart="67dp"
        android:layout_marginTop="86dp"
        android:text="Nearest"
        android:textColor="#FFFFFF"
        android:textSize="12sp"
        android:textStyle="normal"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />


    <TextView
        android:id="@+id/textView3"
        android:layout_width="78dp"
        android:layout_height="15dp"
        android:layout_marginStart="130dp"
        android:layout_marginTop="86dp"
        android:text="Upcoming"
        android:textColor="@color/white"
        android:textSize="12sp"
        android:textStyle="normal"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />


    <TextView
        android:id="@+id/textView4"
        android:layout_width="45dp"
        android:layout_height="15dp"
        android:layout_marginStart="224dp"
        android:layout_marginTop="86dp"
        android:text="Past"
        android:textColor="@color/white"
        android:textSize="12sp"
        android:textStyle="normal"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <androidx.cardview.widget.CardView
        android:id="@+id/cardView"
        android:layout_width="320dp"
        android:layout_height="430dp"
        android:layout_marginStart="20dp"
        android:layout_marginTop="125dp"
        android:layout_marginEnd="20dp"
        app:cardBackgroundColor="#171717"
        app:cardCornerRadius="10dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <TextView
                android:layout_width="80dp"
                android:layout_height="29dp"
                android:layout_marginStart="32dp"
                android:layout_marginTop="189dp"
                android:backgroundTint="@color/black"
                android:gravity="center"
                android:text="City Name"
                android:textColor="@color/white"
                android:textSize="12sp"
                android:textStyle="bold"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

            <TextView
                android:layout_width="88dp"
                android:layout_height="23dp"
                android:layout_marginStart="205dp"
                android:layout_marginTop="189dp"
                android:layout_marginEnd="27dp"
                android:gravity="center"
                android:text="State Name"
                android:textColor="@color/white"
                android:textSize="12sp"
                android:textStyle="bold"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />
        </androidx.constraintlayout.widget.ConstraintLayout>

        <TextView
            android:id="@+id/RideId"
            android:layout_width="264dp"
            android:layout_height="22dp"
            android:layout_marginStart="29dp"
            android:layout_marginTop="228dp"
            android:text="Ride Id:"
            android:textColor="#CFCFCF"
            android:textStyle="normal" />

        <TextView
            android:id="@+id/OriginStation"
            android:layout_width="264dp"
            android:layout_height="22dp"
            android:layout_marginStart="29dp"
            android:layout_marginTop="262dp"
            android:text="Origin Station:"
            android:textColor="#CFCFCF"
            android:textStyle="normal" />


        <TextView
            android:id="@+id/StationPath"
            android:layout_width="264dp"
            android:layout_height="22dp"
            android:layout_marginStart="29dp"
            android:layout_marginTop="296dp"
            android:text="Station Path:"
            android:textColor="#CFCFCF"
            android:textStyle="normal" />

        <TextView
            android:id="@+id/Date"
            android:layout_width="264dp"
            android:layout_height="22dp"
            android:layout_marginStart="29dp"
            android:layout_marginTop="352dp"
            android:text="Date:"
            android:textColor="#CFCFCF"
            android:textStyle="normal" />

        <TextView
            android:id="@+id/Distance"
            android:layout_width="264dp"
            android:layout_height="22dp"
            android:layout_marginStart="29dp"
            android:layout_marginTop="386dp"
            android:text="Distance:"
            android:textColor="#CFCFCF"
            android:textStyle="normal" />


    </androidx.cardview.widget.CardView>


</androidx.constraintlayout.widget.ConstraintLayout>